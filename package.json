{
  "name": "Test-Front",
  "version": "1.0.0",
  "description": "Learning React  Redux Next js",
  "main": "server/index.js",
  "scripts": {
    "dev": "node .",
    "build": "next build",
    "stop": "pm2 stop TestFront",
    "start": "NODE_ENV=production NVM_DIR=~/.nvm pm2 start pm2.json",
    "restart": "$npm_execpath run build && $npm_execpath run stop && NODE_ENV=production NVM_DIR=~/.nvm pm2 start pm2.json",
    "clean": "rm -rf node_modules && rm -rf .docz && rm -rf .next && rm -rf yarn-error.log && rm -rf package-lock.json && $npm_execpath install",
    "prod": "$npm_execpath run build && NODE_ENV=production node .",
    "analyze": "ANALYZE=1 next build",
    "analyze-log": "ANALYZE_LOG=1 next build && cat .next/stats.txt",
    "autofix": "prettier-eslint --eslint-config-path .eslintrc --config .prettierrc --write \"{common-util,components,helpers,pages,server,store,util}/**/*.js\" && prettier-eslint --eslint-config-path .eslintrc --config .prettierrc --write \"{common-util,components,helpers,pages,server,store,util}/**/*.jsx\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruchidh/Tiger-Project.git"
  },
  "keywords": [],
  "author": "Ruchi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruchidh/Tiger-Project/issues"
  },
  "homepage": "https://github.com/ruchidh/Tiger-Project#readme",
  "dependencies": {
    "antd": "^3.11.6",
    "axios": "^0.18.0",
    "checkit": "^0.7.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "moment": "^2.23.0",
    "next": "^7.0.2",
    "next-redux-wrapper": "^2.1.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-datepicker": "^2.0.0",
    "react-dom": "^16.7.0",
    "react-popover": "^0.5.10",
    "react-redux": "^6.0.0",
    "reactjs-toastr": "^1.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "styled-components": "^4.1.3",
    "styled-normalize": "^8.0.4"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.10.0",
    "circular-dependency-plugin": "^5.0.2",
    "eslint": "^5.11.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "prettier-eslint-cli": "^4.7.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-bundle-size-analyzer": "^3.0.0"
  }
}
